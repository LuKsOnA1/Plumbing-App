@model List<ContactVMForUI>


<section id="contact" class="contact">
	<div class="container">

		<div class="section-title">
			<span>Contact</span>
			<h2>Contact</h2>
		</div>

		<div class="row">

			<div class="d-flex align-items-stretch">
				<div class="info">

					@foreach(var item in Model)
					{
						<div class="address">
							<i class="bi bi-geo-alt"></i>
							<h4>Location:</h4>
							<p>@item.Location</p>
						</div>

						<div class="email">
							<i class="bi bi-envelope"></i>
							<h4>Email:</h4>
							<p>@item.Email</p>
						</div>

						<div class="phone">
							<i class="bi bi-phone"></i>
							<h4>Call:</h4>
							<p>@item.Call</p>
						</div>

						<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d23829.150998049245!2d44.73963240592034!3d41.70662305003418!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x404473360c6f999b%3A0x25172c924c997550!2sVake%2C%20Tbilisi!5e0!3m2!1sen!2sge!4v1710078110424!5m2!1sen!2sge" frameborder="0" style="border:0; width: 100%; height: 290px;"></iframe>
					}

				</div>

			</div>

		</div>

	</div>
</section>
@model TeamUpdateVM

<div class="col-md-3">
	<div class="form-floating">
		<input asp-for="Twitter" type="text" class="form-control" placeholder="Twitter">
		<label asp-for="Twitter">Twitter</label>
		<span asp-validation-for="Twitter" class="text-danger"></span>
	</div>
</div>
<div class="col-md-3">
	<div class="form-floating">
		<input asp-for="Facebook" type="text" class="form-control" placeholder="Facebook">
